<p id="notice"><%= notice %></p>

<h1>Targets</h1>

<table>
  <thead>
    <tr>
      <th>Vuforia name</th>
      <th>Transaction</th>
      <th>Target</th>
      <th>Image</th>
      <th>Path</th>
      <th>Place</th>
      <th>City</th>
      <th>Discountchance</th>
      <th>Discountrate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @targets.each do |target| %>
      <tr>
        <td><%= target.vuforia_name %></td>
        <td><%= target.transaction_id %></td>
        <td><%= target.target_id %></td>
        <td><%= target.image %></td>
        <td><%= target.path %></td>
        <td><%= target.place %></td>
        <td><%= target.city %></td>
        <td><%= target.discountChance %></td>
        <td><%= target.discountRate %></td>
        <td><%= link_to 'Show', target %></td>
        <td><%= link_to 'Edit', edit_target_path(target) %></td>
        <td><%= link_to 'Destroy', target, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Target', new_target_path %>
